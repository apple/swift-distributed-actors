# file options

--exclude .build
--exclude "**/*.pb.swift"
--exclude "**/*+GenActor.swift"
--exclude "**/*+GenCodable.swift"

# format options

--comments ignore
--ifdef no-indent
--patternlet inline
--self insert
--stripunusedargs closure-only
